<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.shiying.patient_handle.mapper.PatientHandleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.shiying.patient_handle.entity.PatientHandle">
        <id column="id" property="id" />
        <result column="handle_name" property="handleName" />
        <result column="handle_price" property="handlePrice" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, handle_name, handle_price
    </sql>

</mapper>
